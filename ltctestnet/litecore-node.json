{
  "network": "testnet",
  "port": 3001,
  "services": [
    "bitcoind",
    "web",
    "insight-lite-api",
    "insight-lite-ui"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/root/.litecoin/data",
        "exec": "/usr/local/lib/node_modules/litecore/node_modules/litecore-node/bin/litecoind"
      },
      "insight-lite-api": {
        "disableRateLimiter": true,
        "rateLimiterOptions": {
          "limit": 1000000000000
        }
      }
    }
  }
}